	 private WebDriver driver;
	 
	    @BeforeAll
	    void setUp() {
	        driver = new ChromeDriver();
	    }
	 
	    @AfterAll 
	    void tearDown() {
	        if (driver != null) {
	            driver.quit();
	        }
	    }
	 
	    @BeforeEach
	    void goToHomePage() {
	    	driver.get("http://localhost:3000/");
	    }
	 
	    @Test
	    void test1() {
	        assertEquals("React App", driver.getTitle());
	    }
	 
